<div class="user-index-wrapper ">
  <div class="container user-index-container">
    <h1>全てのユーザー</h1>
    <div class="row">
      <div class="col-md-10 user-index">
        <% @users.each do |user| %>
          <ul>
            <li class= "text-black">
              <%= image_tag user.image if user.image.attached? %>
              <%= link_to user.name, user %>
            </li>
          </ul>
        <% end %>
      </div>
    </div>
    <div class="paginate">
      <%= paginate @users %>
    </div>
  </div>
</div>
